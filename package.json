{
  "name": "react-boilerplate",
  "version": "0.1.4",
  "description": "a react boilerplate",
  "main": "main.js",
  "scripts": {
    "start": "node ./server.js",
    "mock": "cd ./mock & json-server --watch db.json --routes routes.json --port 9001",
    "lint": "eslint ./src",
    "test": "karma start  --config karma.conf.js",
    "clean": "rimraf dist",
    "deploy": "npm run clean && cross-env NODE_ENV=production webpack --config webpack.config.js"
  },
  "dependencies": {
    "babel-polyfill": "^6.3.14",
    "react": "^0.14.3",
    "react-dom": "^0.14.3"
  },
  "devDependencies": {
    "autoprefixer": "^6.1.1",
    "babel-core": "^6.2.1",
    "babel-loader": "^6.2.0",
    "babel-plugin-transform-es3-member-expression-literals": "^6.3.13",
    "babel-plugin-transform-es3-property-literals": "^6.3.13",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babel-preset-react-hmre": "^1.1.0",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.0",
    "eslint": "^1.10.3",
    "eslint-plugin-react": "^3.11.3",
    "exports-loader": "^0.6.2",
    "extract-text-webpack-plugin": "^0.9.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^1.6.2",
    "imports-loader": "^0.6.5",
    "json-server": "^0.8.4",
    "karma": "^0.13.15",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-chrome-launcher": "^0.2.2",
    "karma-firefox-launcher": "^0.1.7",
    "karma-ie-launcher": "^0.2.0",
    "karma-jasmine": "^0.3.6",
    "karma-webpack": "^1.7.0",
    "opn": "^3.0.3",
    "postcss-filter-gradient": "^0.1.1",
    "postcss-import": "^7.1.3",
    "postcss-loader": "^0.8.0",
    "postcss-url": "^5.0.2",
    "precss": "^1.3.0",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.6",
    "webpack-dev-server": "^1.12.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mingzepeng/react-boilerplate.git"
  },
